     code : |1-
      // Résumé : Test de compatiblité selon le numéro de version
      // Syntaxe :
      //[ <Résultat> = ] gbVersionCompatible (<nProduit> est entier, <nVersion> est entier)
      //
      // Paramètres :
      //	nProduit (entier) : Code du produit
      //	nVersion (entier) : Numéro de version à tester
      // Valeur de retour :
      // 	booléen : Vrai si compatible (version courante du produit ou "préversion"
      //
      // Exemple :
      // SI PAS gbVersionCompatible(PRODUIT_WINDEV, 20) ALORS
      //		IHMNouvelleFonctionnalite..Visible = Faux
      // FIN
      //
      PROCEDURE gbVersionCompatible(nProduit est entier, nVersion est entier) : booléen
      
      //Préfixe du fichier selon le produit
      sPrefixeFichierPreversion est chaine
      selon nProduit
      	cas PRODUIT_WINDEV
      		sPrefixeFichierPreversion = "WD_"
      	cas PRODUIT_WEBDEV
      		sPrefixeFichierPreversion = "WB_"
      	cas PRODUIT_WINDEVPOCKET
      		sPrefixeFichierPreversion = "WM_"
      		
      	//Autre produit : le fichier de préversion n'est pas géré
      	AUTRE CAS
      		//Cas par défaut : comparaison simple de version majeure
      		renvoyer nVersion <= nXVER_MAJOR
      FIN
      
      soit sCheminFichierPreversion = fRepExe() + [fsep] + sPrefixeFichierPreversion + "Preversion.txt"
      si pas fFichierExiste(sCheminFichierPreversion) ALORS
      	//Cas par défaut : comparaison simple de version majeure
      	RENVOYER nVersion <= nXVER_MAJOR
      FIN
      
      soit sChainePreversion = fChargeTexte(sCheminFichierPreversion)
      
      si pas (ExtraitChaîne(sChainePreversion, 3,"#") [~ "Preversion") ALORS
      	//Cas par défaut : comparaison simple de version majeure
      	RENVOYER nVersion <= nXVER_MAJOR
      FIN
      
      nPreVersionMajeure est entier = ExtraitChaîne(sChainePreversion,1 ,"#")
      si nPreVersionMajeure < nXVER_MAJOR ALORS
      	nPreVersionMajeure = nXVER_MAJOR
      FIN
      
      renvoyer nVersion <= nPreVersionMajeure
