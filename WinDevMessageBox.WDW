      procedure WinDevMessageBox( szTitre="Test", szLibelle="", nType=1, nNumBouton=1)
      // Fen√™tre alternative √† la MessageBox standard de WINDOWS.
      // Cette fen√™tre est rajout√©e au projet si l'option 
      // "[] appliquer le th√®me du gabarit aux fen√™tres syst√®me" est coch√©e.
      // Elle est utilis√©e pour les fonctions suivantes :
      // -Info()
      // -Erreur()
      // -OuiNon()
      // -Confirmer()
      // -Avertissement()
      // -OKAnnuler()
      // Param√®tres:
      //  szTitre   :  Titre que l'on veut donner √† la fen√™tre
      //  szLibelle :  Le message √† afficher dans la fen√™tre
      //  nType     :  Le dialogue √† √©muler : 0 = Info, 1 = Erreur, 2=OuiNon, 3=Confirmer, 4=Avertissement, 5=OKAnnuler.
      //  nNumBouton:  n¬∞ de bouton √† s√©lectionner (pour OuiNon et Confirmer)
      //
      
      global
      	// Largeur pour les boutons du dialogue
      	// ( Calcul√© selon le type de dialogue )
      	nLargeurBtn est entier = 0
      // marge en plus des boutons pour la largeurMin
      constant MARGEBORDS  = 20
      // marge entre les boutons
      constant MARGEINTERBTN  = 6
      
      // constantes renvoy√©es par MessageBox
      CONSTANT IDOK     =           1
      CONSTANT IDCANCEL =  	      2
      CONSTANT IDABORT  =           3
      CONSTANT IDRETRY  =           4
      CONSTANT IDIGNORE =           5
      CONSTANT IDYES    =           6
      CONSTANT IDNO     =           7
      
      
      
      // on met en place le message
      LIB_INFO = szLibelle
      // on met en place le titre
      moimeme..Titre = szTitre
      
      // on met en place les boutons
      selon nType
      	cas 0 : // Info
      		// on rend visible le bouton [OK] et l'ic√¥ne "I"
      		BTN_OK..Visible = VRAI	
      		ICON_INFORMATION..Visible = VRAI	
      		// la largeur est celle d'un bouton
      		nLargeurBtn = BTN_OK..Largeur 
      		// Valeur renvoy√©e si on ferme par la croix : OK
      		moimeme..ValeurRenvoy√©e = IDOK
      	cas 1 : // Erreur
      		// on rend visible le bouton [OK]et  l'ic√¥ne "x"
      		BTN_OK..Visible = VRAI
      		ICON_ERREUR..Visible = VRAI
      		// la largeur est celle d'un bouton
      		nLargeurBtn =  BTN_OK..Largeur 
      		// Valeur renvoy√©e si on ferme par la croix : OK
      		moimeme..ValeurRenvoy√©e = IDOK		
      	cas 2 : // OuiNon
      		// on rend visible les boutons [OUI] et [NON] et  l'ic√¥ne "?"
      		BTN_OUI..Visible = VRAI		
      		BTN_NON..Visible = VRAI		
      		ICON_QUESTION..Visible = VRAI	
      		// la largeur est celle de 2 boutons + la marge 
      		nLargeurBtn = BTN_OUI..Largeur *2 + MARGEINTERBTN
      		// si on veut s√©lectionner [NON] comme bouton par d√©faut
      		SI nNumBouton=2 alors EcranPremier(BTN_NON)
      		// Valeur renvoy√©e si on ferme par la croix : NON
      		moimeme..ValeurRenvoy√©e = IDNO			
      	cas 3 : // Confirmer
      		// on rend visible les boutons [OUI], [NON] et [ANNULER] et  l'ic√¥ne "?"
      		BTN_OUI..Visible = VRAI		
      		BTN_NON..Visible = VRAI		
      		BTN_ANNULER..Visible = VRAI	
      		ICON_QUESTION..Visible = VRAI	
      		// la largeur est celle de 3 boutons + la marge 
      		nLargeurBtn =  BTN_OUI..Largeur *3 + MARGEINTERBTN*2
      		// si on veut s√©lectionner [NON] comme bouton par d√©faut
      		SI nNumBouton=2 alors EcranPremier(BTN_NON)
      		// si on veut s√©lectionner [Annuler] comme bouton par d√©faut
      		SI nNumBouton=3 alors EcranPremier(BTN_ANNULER)
      		// Valeur renvoy√©e si on ferme par la croix : ANNULER
      		moimeme..ValeurRenvoy√©e = IDCANCEL	
      	cas 4 : // Avertissement
      		// on rend visible le bouton [OK] et l'ic√¥ne "!"
      		BTN_OK..Visible = VRAI	
      		ICON_INFORMATION..Visible = VRAI	
      		// la largeur est celle d'un bouton
      		nLargeurBtn = BTN_OK..Largeur 
      		// Valeur renvoy√©e si on ferme par la croix : OK
      		moimeme..ValeurRenvoy√©e = IDOK	
      	cas 5 : // OuiNon
      		// on rend visible les boutons [OUI] et [NON] et  l'ic√¥ne "I"
      		BTN_OUI..Visible = VRAI		
      		BTN_NON..Visible = VRAI		
      		ICON_QUESTION..Visible = VRAI	
      		// Met le libell√© "OK" sur [OUI] et "Annuler" sur [NON]
      		BTN_OUI..Libell√© = BTN_OK..libell√©
      		BTN_NON..Libell√© = BTN_ANNULER..Libell√©
      		// la largeur est celle de 2 boutons + la marge 
      		nLargeurBtn = BTN_OUI..Largeur *2 + MARGEINTERBTN
      		// si on veut s√©lectionner [Annuler] comme bouton par d√©faut
      		SI nNumBouton=2 alors EcranPremier(BTN_NON)
      		// Valeur renvoy√©e si on ferme par la croix : NON
      		moimeme..ValeurRenvoy√©e = IDNO								
      FIN
      
      // calcule la largeur et la hauteur de la boite de dialogue
      RedimensionneDialogue()
      
      // centre les boutons 
      CentreLesBoutons()
      
      // on √©met un avertissement sonore (comme le syst√®me)
      AvertissementSonore()
      
      
